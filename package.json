{
  "name": "cloudflare-workers-project",
  "version": "1.0.0",
  "description": "Cloudflare Workers项目，兼容Ubuntu 20.04",
  "type": "module",
  "main": "lib/index.js",
  "scripts": {
    "sync": "cp -r ~/create/template/lib/model/* lib/model",
    "sync-back": "cp -r lib/model/* ~/create/template/lib/model",
    "build-routes": "node scripts/build-routes.mjs",
    "prebuild": "pnpm run build-routes",
    "predev": "pnpm run build-routes",
    "dev": "dotenv -- vite dev",
    "build": "dotenv -- vite build",
    "preview": "dotenv -- vite build && wrangler dev",
    "deploy": "dotenv -- vite build && wrangler deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
